{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/home", "destination": "/index.html" },
    { "source": "/:path*", "destination": "/:path*.html" }
  ],
  "redirects": [
    { "source": "/index", "destination": "/home", "permanent": true },
    { "source": "/:path*.html", "destination": "/:path*", "permanent": true }
  ]
}
